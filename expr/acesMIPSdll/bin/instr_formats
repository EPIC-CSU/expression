FORMAT _UNIT_ _SOURCE_1_ _SOURCE_2_ _SOURCE_3_ _DEST_
	_SOURCE_1_ -> _DEST_
	_SOURCE_2_ -> _DEST_
	_SOURCE_3_ -> _DEST_
FORMAT _UNIT_ _SOURCE_1_ _SOURCE_2_ _DEST_
	_SOURCE_1_ -> _DEST_
	_SOURCE_2_ -> _DEST_
FORMAT _UNIT_ _SOURCE_1_ _DEST_
	_SOURCE_1_ -> _DEST_
FORMAT _UNIT_ _SOURCE_1_ _SOURCE_2_ _MEM_BYTE_ 
	_SOURCE_1_ -> _MEM_BYTE_
	_SOURCE_2_ -> _MEM_BYTE_
FORMAT _UNIT_ _SOURCE_1_ _DEST_ _MEM_BYTE_ 
	_SOURCE_1_ -> _MEM_BYTE_
	_MEM_BYTE_ -> _DEST_

